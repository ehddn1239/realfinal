<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdm.fj.etc.EtcMapper">

	<select id="showAllAccessory" resultType="com.sdm.fj.etc.EtcDTO">
		select * from
		final_product where p_category=13

	</select>

	<select id="showAllBag" resultType="com.sdm.fj.etc.EtcDTO">
		select * from final_product
		where p_category=14

	</select>
	
	<select id="showAllShoes" resultType="com.sdm.fj.etc.EtcDTO">
		select * from final_product
		where p_category=15

	</select>

</mapper>