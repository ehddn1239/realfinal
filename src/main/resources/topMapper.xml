<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdm.fj.top.TopMapper">
	<select id="showAllTopKnitwear" resultType="com.sdm.fj.top.TopDTO">
	select * from final_product where p_category = 4
	</select>
	
	<select id="showAllTopShirt" resultType="com.sdm.fj.top.TopDTO">
	select * from final_product where p_category = 5
	</select>
	
	<select id="showAllTopTshirt" resultType="com.sdm.fj.top.TopDTO">
	select * from final_product where p_category = 6
	</select>
	
	<select id="showAllTopDress" resultType="com.sdm.fj.top.TopDTO">
	select * from final_product where p_category = 7
	</select>
</mapper>