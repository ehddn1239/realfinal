<mapper namespace="com.sdm.fj.bottom.BottomMapper">

	<select id="showAllPants"
		resultType="com.sdm.fj.bottom.BottomDTO">
		select * from final_product where p_category=9
	</select>

	<select id="showAllDenim"
		resultType="com.sdm.fj.bottom.BottomDTO">
		select * from final_product where p_category=10
	</select>

	<select id="showAllSkirt"
		resultType="com.sdm.fj.bottom.BottomDTO">
		select * from final_product where p_category=11
	</select>
	
	<select id="showAllShorts"
		resultType="com.sdm.fj.bottom.BottomDTO">
		select * from final_product where p_category=12
	</select>
</mapper>